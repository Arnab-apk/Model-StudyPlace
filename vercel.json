{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/chat",
            "destination": "/api/chat.js"
        },
        {
            "source": "/engine",
            "destination": "/viewer.html"
        },
        {
            "source": "/heart",
            "destination": "/viewer.html"
        },
        {
            "source": "/dna",
            "destination": "/viewer.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}